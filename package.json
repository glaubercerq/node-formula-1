{
  "name": "formula-one-api",
  "version": "1.0.0",
  "description": "Formula 1 API with teams and drivers data",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsup src",
    "dev": "tsx --env-file=.env src/server.ts",
    "watch": "tsx watch --env-file=.env src/server.ts",
    "start": "npm run build && node dist/server.js",
    "preview": "node dist/server.js"
  },
  "keywords": ["formula1", "f1", "api", "fastify", "typescript"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "fastify": "^4.26.2"
  }
}
